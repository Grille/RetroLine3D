    "compilerOptions": {
        "target": "ES2020",
        "module": "ES2020",
        "outDir": "out/src",
        "rootDir": "src",
        "strict": true,
        "alwaysStrict": true,
        "strictFunctionTypes": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "allowArbitraryExtensions": false
    }
